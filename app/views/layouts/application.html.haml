!!!
%html
  %head
    = render 'layouts/head'
    = yield(:head_javascripts)
  %body
    - if admin_signed_in?
      = render 'layouts/admin_masthead'
    - else
      = render 'layouts/masthead'
    .container-fluid
      = render 'layouts/messages'
      = yield
    = render 'layouts/tail'
    = render 'layouts/debug' if Rails.env.development?
    = yield(:javascripts)
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'